@model IEnumerable<StudentReg.Models.Courses>

<p >
    Запись студента на курсы: @ViewData["StudentName"]
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CoursesName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CoursesName)
            </td>
            <td>
                <a asp-action="AddedToCourse" asp-route-id="@item.CoursesID" asp-route-studId ="@ViewData["StudentId"]">Добавить</a> |
            </td>
        </tr>
}
    </tbody>
</table>
